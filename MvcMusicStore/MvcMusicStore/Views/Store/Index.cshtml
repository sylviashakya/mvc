@model IEnumerable<MvcMusicStore.Models.Genre>
           

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Browse through these music:</h2>
<p>Select from @Model.Count() genres: </p>
<ul>
    @foreach(var genre in  Model)
    {
       <li>@genre.Name</li>
    }
</ul>
